<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="346.0" prefWidth="563.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.panko.apod.controller.PictureDescriptionController">
    <Text fx:id="pictureTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
        <font>
          <Font name="System Bold" size="14.0"/>
        </font>
        <VBox.margin>
          <Insets bottom="10.0" left="10.0" top="10.0"/>
        </VBox.margin>
    </Text>
    <HBox>
        <TextArea fx:id="pictureDescription" editable="false" prefHeight="338.0" prefWidth="616.0" maxHeight="Infinity"
                  maxWidth="Infinity"
                  wrapText="true">
            <font>
                <Font name="sans-serif" size="14.0"/>
            </font>
        </TextArea>
    </HBox>
</VBox>
